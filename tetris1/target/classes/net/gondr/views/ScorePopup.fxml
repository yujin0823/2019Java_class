      <Button layoutX="333.0" layoutY="48.0" mnemonicParsing="false" onAction="#record" prefHeight="40.0" prefWidth="60.0" text="기록" />
      <Label fx:id="lblScore" layoutX="14.0" layoutY="14.0" prefHeight="15.0" prefWidth="376.0" text="당신의 점수는 0점 입니다">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblError" layoutX="14.0" layoutY="102.0" prefHeight="15.0" prefWidth="376.0" text="error" textFill="RED" />
      <TextField fx:id="txtName" layoutX="14.0" layoutY="48.0" prefHeight="40.0" prefWidth="311.0" promptText="기록된 이름을 입력하세요" />
   </children>
</AnchorPane>
